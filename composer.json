{
    "name":        "arcanesoft/core",
    "description": "The core package foreach ARCANESOFT's module",
    "keywords":    ["arcanedev", "arcanesoft", "laravel", "core", "support"],
    "homepage":    "https://github.com/ARCANESOFT/Core",
    "authors": [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc"
        }
    ],
    "type":    "library",
    "license": "MIT",
    "require": {
        "php":                    ">=5.6.4",
        "arcanedev/breadcrumbs":  "~3.0",
        "arcanedev/hasher":       "~2.1",
        "arcanedev/laravel-html": "~5.4",
        "arcanedev/notify":       "~3.3",
        "arcanedev/seo-helper":   "~1.3",
        "arcanedev/support":      "~4.0",
        "arcanesoft/contracts":   "~2.0",
        "arcanesoft/settings":    "~0.5"
    },
    "require-dev": {
        "phpunit/phpcov":  "~3.0",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Arcanesoft\\Core\\": "src/"
        },
        "files": ["helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanesoft\\Core\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "testbench": "composer require --dev \"orchestra/testbench=~3.4\""
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
