{
    "name":        "arcanesoft/core",
    "description": "The core package foreach ARCANESOFT's module",
    "keywords":    ["arcanedev", "arcanesoft", "laravel", "core", "support"],
    "homepage":    "https://github.com/ARCANESOFT/Core",
    "authors": [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc"
        }
    ],
    "type":    "library",
    "license": "MIT",
    "require": {
        "php":                    ">=5.5.9",
        "arcanedev/breadcrumbs":  "~2.0",
        "arcanedev/hasher":       "~1.0",
        "arcanedev/laravel-html": "~5.0",
        "arcanedev/notify":       "^3.1.0",
        "arcanedev/seo-helper":   "~0.17",
        "arcanedev/support":      "~3.0",
        "arcanesoft/contracts":   "~1.0",
        "arcanesoft/settings":    "~0.1"
    },
    "require-dev": {
        "orchestra/testbench":    "~3.1.0",
        "phpunit/phpcov":         "~2.0",
        "phpunit/phpunit":        "~4.0|~5.0"
    },
    "autoload": {
        "psr-4": {
            "Arcanesoft\\Core\\": "src/"
        },
        "files": ["helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanesoft\\Core\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
